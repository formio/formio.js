<{{ctx.tag}}
    {% if ( !!ctx.tag.match(/[h, H]\d/) ) { %}
    class="title is-{{ctx.tag.match(/[h, H]\d/)[0][1]}}" {% } %} ref="html"
{% ctx.attrs.forEach(function(attr) { %}
    {{attr.attr}}="{{attr.value}}"
  {% }) %}
>{{ctx.content}}{% if (!ctx.singleTags || ctx.singleTags.indexOf(ctx.tag) === -1) { %}</{{ctx.tag}}>{% } %}
