{
  "parser": "babel-eslint",
  "extends": "formio",
  "globals": {
    "Formio": true,
    "FormioForm": true,
    "FormioPDF": true,
    "FormioWizard": true,
    "FormioUtils": true
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "no-console": "off",
    "prefer-arrow-callback": "error",
    "prefer-const": "error",
    "prefer-template": "error",
    "indent": ["error", 2, {
      "SwitchCase": 1
    }]
  }
}
